# E-University Lab Environment Variables
# Copy this file to .env and fill in your credentials
# Generate secure passwords with: python3 -c "import secrets; print(secrets.token_urlsafe(16))"

# NetBox Configuration (optional)
NETBOX_URL=https://your-instance.netboxapp.com
NETBOX_TOKEN=your-api-token-here

# Device Credentials (required for pyATS and telemetry)
DEVICE_USERNAME=admin
DEVICE_PASSWORD=<your-device-password>
DEVICE_ENABLE_PASSWORD=<your-enable-password>

# SNMP Configuration
SNMP_COMMUNITY=<your-snmp-community-string>

# Routing Protocol Authentication (optional but recommended for production)
# Generate with: python3 -c "import secrets; print(secrets.token_hex(16))"
OSPF_AUTH_KEY=<your-ospf-md5-key>
BGP_AUTH_KEY=<your-bgp-md5-key>

# EVE-NG server (optional)
EVE_USERNAME=admin
EVE_PASSWORD=<your-eve-password>
